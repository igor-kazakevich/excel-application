{
  "parser": "babel-eslint",
  "env": {
    "es6":     true,
    "browser": true,
    "node":    true
  },
  "rules": {
    "no-multi-spaces":    "off",
    "arrow-parens":       "off",
    "comma-dangle":       "off",
    "require-jsdoc":      "off",
    "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended", "google"]
}